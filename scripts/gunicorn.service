[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=root
Group=www-data
WorkingDirectory=/var/lib/jenkins_home/workspace/movies-website-ci-cd/
ExecStart=/bin/bash /var/lib/jenkins_home/workspace/movies-website-ci-cd/scripts/gunicorn.service

[Install]
WantedBy=multi-user.target
